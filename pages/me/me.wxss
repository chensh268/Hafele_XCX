/* pages/me/me.wxss */
.panel{
  padding-top: 10px;
  padding-left: 10px;
  padding-right: 10px;
  padding-bottom: 10px;
  margin-left: 3%;
  background: #fff;
  width: 89%;
  margin-top: 30px;
  height: 210px;
  overflow: hidden;
  cursor: move;
  position: relative;
  color: #666;
  border-radius: 5px;
}

.panel .logo{
  text-align: center;
}

.panel .logo image{
  width: 130px;
  height: 35px;
}
.form{
  margin-left: 25px;
  margin-top: 15px;
}

.form .account{
  width: 89%;
  height: 42px;
  margin-bottom: 10px;
}

.form .password{
  width: 89%;
  height: 42px;
  margin-bottom: 10px;
}

.form image{
  float: left;
  width: 42px;
  height: 42px;
}

.form input{
  font-size: 14px;
  width: 80%;
  height: 38px;
  border: 1px solid #000;
}

.button{
  margin-left: 75px;
  width: 55%;
  height: auto;
}

.button button{
  background-color: #9ed269;
}